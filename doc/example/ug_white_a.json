{
    "title": "White Noise Masking",
    "label": "ug_white-mask_select",
    "register": {
        "clock": ["Clock", { "value": 10.0, "mode": "Samples" }],
        "sel-upper": ["Select", {"values": [10, 20, 50], "mode": "Cycle"}],
        "sel-lower": ["Select", {"values": [5, -15, -20, 0], "mode": "Cycle"}],
        "sig": ["White", { "seed": null }]
    },
    "connect": {
        "clock.out": "sel-upper.trigger",
        "clock.out": "sel-lower.trigger"
    }
}

