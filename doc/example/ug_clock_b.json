{
    "title": "Clock Mixture",
    "label": "ug_clock-mix",
    "register": {
        "clock_a": ["Clock", { "value": 12.0, "mode": "Samples" }],
        "clock_b": ["Clock", { "value": 5.0, "mode": "Samples" }],
        "mix": ["Sum", { "input_count": 2 }]
    },
    "connect": {
      "clock_a.out": "mix.in1",
      "clock_b.out": "mix.in2"
    }
}
